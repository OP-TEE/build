/dts-v1/;
/plugin/;

&{/} {
	reserved-memory {
		#address-cells = <2>;
		#size-cells = <2>;

		tfa: tf-a@1600000 {
			no-map;
			reg = <0 0x1600000 0 0x200000>;
		};

		transfer_list: memory@1000000 {
			no-map;
			reg = <0 0x1000000 0 0x600000>;
		};
	};
};
